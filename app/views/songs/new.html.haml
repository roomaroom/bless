.title_wrap
  .container
    .row
      .col-md-10.col-sm-9.col-xs-8
        %h1 Sign Up

.container.content
  = simple_form_for(@song, html: { class: 'form-vertical' }) do |f|
    = f.error_notification
    = f.input :name
    = f.input :audio

    = f.button :submit, 'Створити', class: 'btn-primary'

